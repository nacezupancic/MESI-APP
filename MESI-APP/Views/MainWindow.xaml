<Window x:Class="MESI_APP.Views.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Canvas>
        <Button Margin="20,20" Command="{Binding SaveSettingsCommand}" Content="Save settings" />
        <TextBox Text="{Binding ServerOutboundUrl}" Margin="50" Width="100" FontSize="15" Background="LightBlue" />
    </Canvas>
</Window>
